{
  "name": "@pleme-io/types",
  "version": "1.0.1",
  "description": "Shared TypeScript library for Nexus products",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "tsdown",
    "build:watch": "tsdown --watch",
    "dev": "tsdown --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "check": "biome check --diagnostic-level=warn src",
    "check:types": "tsc --noEmit",
    "format": "biome check --write src",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=18.3.0",
    "react-dom": ">=18.3.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitest/coverage-v8": "^3.2.0",
    "@vitest/ui": "^3.2.0",
    "@biomejs/biome": "^1.9.4",
    "jsdom": "^27.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.0.0",
    "tsdown": "^0.2.0",
    "typescript": "^5.7.0",
    "vite": "^5.0.0",
    "vitest": "^3.2.0"
  },
  "keywords": [
    "react",
    "typescript",
    "library",
    "nexus",
    "pleme"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pleme-io/pleme-types.git"
  },
  "license": "MIT",
  "author": "Pleme.io Team",
  "module": "./dist/index.js"
}
